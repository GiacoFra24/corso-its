generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Character {
  id      String  @id @default(uuid())
  name    String  @db.VarChar(20)
  history String?
  age     Int?
  health  Int?
  stamina Int?
  mana    Int?
}

model Attribute{
  id         String  @id @default(uuid())
  key   String  @db.VarChar(20)
  name       String  @db.VarChar(20)
  value      Int?
}